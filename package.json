{
  "private": true,
  "version": "1.2.0",
  "scripts": {
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "watch-poll": "npm run watch -- --watch-poll",
    "build:dev": "webpack --display-error-details --config webpack.config.js --progress --color",
    "build:prod": "webpack --env.prod --config webpack.config.js --progress --color",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --inline --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "lint-js": "eslint -f pretty --ext .js,.vue resources/js/ ./*.js",
    "lint-css": "stylelint \"resources/sass/**/*.scss\"",
    "fix-js": "eslint -f pretty  --ext .js,.vue resources/js/ ./*.js --fix",
    "fix-css": "stylelint \"resources/sass/**/*.scss\" --fix",
    "lint-php": "php php-cs-fixer fix --dry-run",
    "fix-php": "php php-cs-fixer fix",
    "lint": "npm run lint-js && npm run lint-css && npm run lint-php",
    "fix": "npm run fix-js && npm run fix-css && npm run fix-php",
    "start": "node server.js"
  },
  "engines": {
    "node": ">= 8.9.4",
    "npm": ">= 5.6.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "babel-plugin-wildcard": "^5.0.0",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.2.1",
    "eslint": "^5.16.0",
    "eslint-config-adenvt": "^3.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-formatter-pretty": "^2.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-unicorn": "^9.1.1",
    "eslint-plugin-varspacing": "^1.2.1",
    "eslint-plugin-vue": "^5.2.3",
    "laravel-mix": "^4.1.4",
    "node-sass": "^4.12.0",
    "offline-plugin": "^5.0.7",
    "pre-commit": "^1.2.2",
    "prettier": "1.17.1",
    "resolve-url-loader": "^3.1.0",
    "sass": "^1.22.12",
    "sass-loader": "^7.3.1",
    "stylelint": "^10.1.0",
    "stylelint-config-sass-guidelines": "^6.1.0",
    "stylelint-scss": "^3.11.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "vue-loader": "15.7.0",
    "vue-template-compiler": "^2.6.10",
    "vuetable-2": "^1.7.5",
    "webpackbar": "^3.2.0"
  },
  "dependencies": {
    "@coreui/vue": "^2.1.2",
    "animate.css": "^3.7.2",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.3.1",
    "bootstrap-vue": "^2.0.1",
    "chart.js": "^2.8.0",
    "express": "^4.17.1",
    "flag-icon-css": "^3.4.0",
    "font-awesome": "^4.7.0",
    "jquery": "^3.4.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "node-libcurl": "^2.0.1",
    "node-pre-gyp": "^0.13.0",
    "paymaya-node-sdk": "^0.1.0",
    "popper.js": "^1.15.0",
    "pug": "^2.0.4",
    "select2": "^4.0.10",
    "select2-bootstrap4-theme": "^1.0.0",
    "simple-line-icons": "^2.4.1",
    "stripe": "^7.9.1",
    "sweetalert": "^2.1.2",
    "validate.js": "^0.13.1",
    "vue": "^2.6.10",
    "vue-chartjs": "^3.4.2",
    "vue-loading-overlay": "^3.2.0",
    "vue-loading-spinner": "^1.0.11",
    "vue-notification": "^1.3.16",
    "vue-router": "^3.1.3",
    "vue-stripe-checkout": "^1.2.6",
    "vue-sweetalert2": "^2.1.1",
    "vuejs-datepicker": "^1.6.2",
    "vuelidate": "^0.7.4",
    "vuex": "^3.1.1",
    "vuex-easy-access": "^3.1.7",
    "webpack": "^4.40.2"
  },
  "pre-commit": [
    "lint"
  ]
}
